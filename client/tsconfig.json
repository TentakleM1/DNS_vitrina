{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext", "webworker"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": "./src",
    "paths": {
      "api": ["./shared/api/index"],
      "httpt": ["./shared/httptransport/HTTPTransport"],
      "constant": ["./shared/constant/index"],
      "pages": ["./shared/pages/index"],
      "store": ["./shared/store/store"],
    },
  },
  "include": ["src"]
}
